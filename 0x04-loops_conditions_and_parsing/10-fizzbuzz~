#!/usr/bin/env bash
# Displays numbers from 1 to 10
# Fizzbuzz if a multiple of both 3 and 5
# Fizz if a multiple of 3 and Buzz if a multiple of 5
for number in {1..100}
do
    if (( number % 3 == 0 && number % 5 == 0))
    then
	echo "FizzBuzz"
    elif (( number % 3 == 0))
    then
	echo "Fizz"
    elif (( number % 5 == 0))
    then
	echo "Buzz"
    else
	echo "$number"
    fi
done